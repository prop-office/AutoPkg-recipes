<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
	<key>Description</key>
	<string>Uses parent download recipe to download the latest unlicensed Microsoft Office 365 Suite installer package.</string>
	<key>Identifier</key>
	<string>com.github.prop-office.autopkg-recipes.pkg.MicrosoftOffice365Suite</string>
	<key>Input</key>
	<dict>
        <key>PACKAGES</key>
        <string>/Users/admin/Library/AutoPkg/Packages</string>
	</dict>
	<key>MinimumVersion</key>
	<string>1.0.0</string>
	<key>ParentRecipe</key>
	<string>com.github.prop-office.autopkg-recipes.download.MicrosoftOffice365Suite</string>
	<key>Process</key>
	<array>
        <dict>
            <key>Processor</key>
            <string>PkgCopier</string>
            <key>Arguments</key>
            <dict>
                <key>source_pkg</key>
                <string>%pathname%</string>
            </dict>
        </dict>
        <dict>
            <key>Processor</key>
            <string>Copier</string>
            <key>Arguments</key>
            <dict>
                <key>source_path</key>
                <string>%RECIPE_CACHE_DIR%/Microsoft_Office_365_Suite_%version%.pkg</string>
                <key>destination_path</key>
                <string>%PACKAGES%/Microsoft_Office_365_Suite_%version%.pkg</string>
            </dict>
        </dict>
        <dict>
            <key>Processor</key>
            <string>Copier</string>
            <key>Arguments</key>
            <dict>
                <key>source_path</key>
                <string>%PACKAGES%/Microsoft_Office_365_Suite_%version%.pkg</string>
                <key>destination_path</key>
                <string>%PACKAGES%/Microsoft_Office_365_Suite.pkg</string>
            </dict>
        </dict>
    </array>
</dict>
</plist>